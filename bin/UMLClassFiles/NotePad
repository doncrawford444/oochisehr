o : UndoManager = new UndoManagerundoJButton : JButton = new JButton(" Undo ")redoJButton : JButton = new JButton(" Redo ")findJButton : JButton = new JButton(" Find ")replaceJButton : JButton = new JButton(" Replace ")threeDHumanJPanel : JPanel = new JPanel(gb)threeDHumanJMenu : JMenu = new JMenu(" Anatomy ")fontJPanel : JPanel = new JPanel(gb)fontSizeJLabel : JLabel = new JLabel(" Font size: ")fontSizeJTextField : JTextField = new JTextField(3)arielJButton : JButton = new JButton(" Ariel ")serifJButton : JButton = new JButton(" Serif ")boldJToggleButton : JToggleButton = new JToggleButton(" Bold ")italicJToggleButton : JToggleButton = new JToggleButton(" Italic ")underlineJToggleButton : JToggleButton = new JToggleButton(" Underline ")_________________________________________________________________________________accept(file : File) : booleangetDescription() : StringactionPerformed(e : ActionEvent) : vgetFontAttributes() : AttributeSetfocusGained(fe : FocusEvent) : vfocusLost(fe : FocusEvent) : vpopupMenuCanceled(arg0 : PopupMenuEvent) : vpopupMenuWillBecomeInvisible(arg0 : PopupMenuEvent) : vpopupMenuWillBecomeVisible(arg0 : PopupMenuEvent) : vresetFont() : vmouseClicked(m : MouseEvent) : vmousePressed(m : MouseEvent) : vmouseEntered(e : MouseEvent) : vmouseExited(e : MouseEvent) : vmouseReleased(m : MouseEvent) : vactionPerformed(e : ActionEvent) : vupdateUndoState() : vactionPerformed(e : ActionEvent) : vupdateRedoState() : vundoableEditHappened(e : UndoableEditEvent) : v// EOF